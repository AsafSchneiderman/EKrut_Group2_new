      <RadioButton fx:id="rduPickup" layoutX="146.0" layoutY="148.0" mnemonicParsing="false" onAction="#SelectVendingMachine" prefHeight="17.0" prefWidth="126.0" text="New pickup Order" />
      <RadioButton fx:id="rduDelivery" layoutX="146.0" layoutY="190.0" mnemonicParsing="false" onAction="#SetDeliveryAddress" prefHeight="17.0" prefWidth="134.0" text="New Delivery Order" />
   </children>
</AnchorPane>
