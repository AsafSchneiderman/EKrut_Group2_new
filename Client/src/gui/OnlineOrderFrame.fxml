      <Button fx:id="btnContinue" layoutX="623.0" layoutY="329.0" mnemonicParsing="false" onAction="#continueToOrder" prefHeight="35.0" prefWidth="91.0" text="Continue">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <ImageView fx:id="imgIcone" fitHeight="82.0" fitWidth="101.0" layoutX="485.0" layoutY="16.0" pickOnBounds="true" preserveRatio="true" />
      <TextField fx:id="txtFldStreet" layoutX="492.0" layoutY="265.0" />
      <Label fx:id="lblCity" layoutX="419.0" layoutY="214.0" prefHeight="17.0" prefWidth="34.0" text="City:" />
      <Label fx:id="lblStreet" layoutX="419.0" layoutY="269.0" text="Street:" />
