<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" prefHeight="450.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.CustomerFrameController">
   <children>
      <Button fx:id="btnExit" layoutX="228.0" layoutY="333.0" mnemonicParsing="false" onAction="#exitProg" text="Exit" textFill="#ee0303" />
      <Button fx:id="bntLocalOrder" layoutX="400.0" layoutY="175.0" mnemonicParsing="false" onAction="#startLocalOrder" prefHeight="25.0" prefWidth="139.0" text="Start new local order" />
      <Button fx:id="bntPickupOrder" layoutX="400.0" layoutY="236.0" mnemonicParsing="false" onAction="#pickupOrder" prefHeight="25.0" prefWidth="139.0" text="Pickup order" />
      <Label fx:id="lblWelcome" layoutX="573.0" layoutY="14.0" text="Welcome" textFill="#e10000">
         <font>
            <Font name="Arial" size="16.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
