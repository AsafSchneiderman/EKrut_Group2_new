          <TableColumn fx:id="regionCol" prefWidth="185.0" text="Region" />
          <TableColumn fx:id="locationCol" minWidth="0.0" prefWidth="202.0" text="Location" />
            <TableColumn fx:id="thresholdLevelCol" minWidth="0.0" prefWidth="168.0" text="Threshold Level" />
        </columns>
      </TableView>
      <Label layoutX="118.0" layoutY="53.0" text="Choose Vending machine to update the Threshold Level:">
         <font>
            <Font name="System Bold" size="15.0" />
         </font></Label>
      <Button fx:id="btnBack" layoutX="118.0" layoutY="396.0" mnemonicParsing="false" onAction="#BackToPreviousPage" text="Back" />
      <Button fx:id="btnUpdateThresholdLevel" layoutX="312.0" layoutY="396.0" mnemonicParsing="false" onAction="#updateThresholdLevel" text="Update Threshold Level" />
