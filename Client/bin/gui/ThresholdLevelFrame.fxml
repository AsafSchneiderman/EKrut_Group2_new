<AnchorPane fx:id="pane" prefHeight="450.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ThresholdLevelFrameController">
   <children>
      <TableView fx:id="tblViewVendingMachines" layoutX="117.0" layoutY="89.0" prefHeight="282.0" prefWidth="556.0">
